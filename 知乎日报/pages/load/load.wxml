<!--pages/load/load.wxml-->
<view class='container'>
  <image src='{{bgurl}}'></image>
  <view class='formGroup'>
  <form  bindsubmit='loadForm'>
    <input bindblur='getName' name='username' placeholder='请输入用户名' type='text'></input>
    <input bindblur='getPassword' name='password' placeholder='请输入密码' type='password'></input>
    <view class='btnGroup'>
      <button form-type='reset' bindtap='setInfor'>注册</button>
      <button form-type='submit'>登录</button>
    </view>
    </form>
  </view>


</view>