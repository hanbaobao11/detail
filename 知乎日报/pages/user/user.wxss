/* pages/user/user.wxss */

page{
  width:100%;
  height:100%;
  overflow-y:hidden; 
}
.container{
  padding:0;
}
.user{
  position: relative;
  width:100%;
  height:200px;
  overflow: hidden;
}
.bgpic{
  width:100%;
  height:200px;
  display: block;
}
.portraitGroup{
  position: absolute;
  left:50%;
  top:50%;
  transform: translate(-50%,-50%);
  display: flex;
  justify-content: center;
}
.userpic{
  width:64px;
  height:64px;
 
  border-radius: 50%;
  background-color: #ccc;
}

